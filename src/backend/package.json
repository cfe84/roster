{
  "name": "roster-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts"
    },
    "bnr": {
      "patterns": [
        "src"
      ],
      "extensions": "ts"
    },
    "test": {
      "patterns": [
        "test",
        "src"
      ],
      "extensions": "ts"
    }
  },
  "scripts": {
    "test": "mocha --require ts-node/register ./test/**/*.ts",
    "watch": "npm-watch",
    "clean": "rm -r dist/",
    "start": "node dist/app.js",
    "bnr": "npm run build && npm run start",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfe84/roster.git"
  },
  "author": "Charles Feval",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cfe84/roster/issues"
  },
  "homepage": "https://github.com/cfe84/roster#readme",
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.17",
    "@types/socket.io": "^2.1.4",
    "mocha": "^6.2.2",
    "npm-watch": "^0.6.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  }
}